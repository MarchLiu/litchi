{
  "title": "Litchi",
  "description": "AI Plugin for JupyterLab.",
  "jupyter.lab.menus": {
    "main": [
      {
        "id": "litchi-menu",
        "label": "Litchi",
        "rank": 80,
        "items": [
          {
            "command": "litchi:chat"
          },
          {
            "command": "litchi:contextual"
          },
          {
            "command": "litchi:historical"
          },
          {
            "command": "litchi:selected"
          },
          {
            "command": "litchi:continuous"
          },
          {
            "type": "separator"
          },
          {
            "command": "litchi:translate",
            "args": {
              "language": "English"
            }
          },
          {
            "command": "litchi:translate",
            "args": {
              "language": "Chinese"
            }
          },
          {
            "type": "separator"
          },
          {
            "command": "litchi:unit-test"
          },
          {
            "type": "separator"
          },
          {
            "command": "litchi:show-roles-toggle"
          },
          {
            "command": "litchi:continuous-toggle"
          }
        ]
      }
    ]
  },
  "jupyter.lab.toolbars": {
    "Notebook": [],
    "Cell": [
      {
        "name": "litchi-continuous",
        "command": "litchi:continuous"
      },
      {
        "name": "litchi-translate-chinese",
        "command": "litchi:translate",
        "args": {
          "language": "Chinese"
        }
      },
      {
        "name": "litchi-translate-english",
        "command": "litchi:translate",
        "args": {
          "language": "English"
        }
      },
      {
        "name": "litchi-unit-test",
        "command": "litchi:unit-test"
      },
      {
        "name": "litchi-split-cell",
        "command": "litchi:split-cell"
      }
    ]
  },
  "properties": {
    "key": {
      "type": "string",
      "title": "Authorization Key:",
      "description": "The AI service API key. If you use a online service as openai, you need set it",
      "default": ""
    },
    "list-models": {
      "type": "string",
      "title": "List Models API:",
      "description": "The AI service URL for list models. The default is ollama in localhost. If you want to connect other service, set it as your need",
      "default": "http://localhost:11434/api/tags"
    },
    "chat": {
      "type": "string",
      "title": "Chat API:",
      "description": "The AI service URL for chat messages. The default is ollama in localhost. If you want to connect other service, set it as your need",
      "default": "http://localhost:11434/api/chat"
    },
    "system": {
      "type": "string",
      "title": "System Prompt:",
      "description": "System Prompt would insert into at first of chat messages list with system role.",
      "default": "your are a python and jupyter export",
      "ui-widget": {
        "ui:widget": "textarea"
      }
    },
    "translators": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "title": "Translate Support:",
      "description": "List the Languages all you need to a translate shortcut",
      "default": []
    },
    "continuous-mode": {
      "type": "boolean",
      "title": "Continuous Mode",
      "description": "Auto generate a markdown cell below the response cell if continuous mode.",
      "default": true
    },
    "providers": {
      "type": "array",
      "title": "AI Providers",
      "description": "AI Service Supply Chat service.",
      "items": {
        "type": "object",
        "anyOf": [
          {
            "title": "Ollama",
            "type": "object",
            "properties": {
              "name": {
                "type": "string",
                "title": "Provider Name",
                "description": "Set a Name will easy to remember if You have many ollama instances"
              },
              "baseUrl": {
                "type": "string",
                "title": "Base Url",
                "default": "http://localhost:11434"
              },
              "category": {
                "type": "string",
                "value": "ollama"
              }
            }
          },
          {
            "title": "OpenAI",
            "type": "object",
            "properties": {
              "name": {
                "type": "string",
                "title": "Provider Name",
                "description": "Set a Name will easy to remember if You have many OpenAI Logins"
              },
              "authKey": {
                "type": "string",
                "title": "Authorization Key"
              },
              "category": {
                "type": "string",
                "value": "openai"
              }
            }
          },
          {
            "title": "KIMI",
            "type": "object",
            "properties": {
              "name": {
                "type": "string",
                "title": "Provider Name",
                "description": "Set a Name will easy to remember if You have many Kimi Logins",
                "default": "KIMI"
              },
              "authKey": {
                "type": "string",
                "title": "Authorization Key"
              },
              "category": {
                "type": "string",
                "value": "kimi",
                "readonly": true
              }
            }
          },
          {
            "title": "DeepSeek",
            "type": "object",
            "properties": {
              "name": {
                "type": "string",
                "title": "Provider Name",
                "description": "Set a Name will easy to remember if You have many DeepSeek Logins",
                "default": "DeepSeek"
              },
              "authKey": {
                "type": "string",
                "title": "Authorization Key"
              },
              "category": {
                "type": "string",
                "value": "deepseek",
                "readonly": true
              }
            }
          }
        ]
      },
      "default": [
        {
          "name": "Ollama",
          "baseUrl": "http://127.0.0.1:11434",
          "category": "ollama"
        }
      ]
    },
    "selected": {
      "type": "string",
      "title": "Select Provider",
      "description": "The AI Provider used for chat",
      "default": "Ollama"
    }
  },
  "additionalProperties": false,
  "type": "object"
}